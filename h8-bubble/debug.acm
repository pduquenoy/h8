DPAGE	EQU	*
	PUSH    PSW
	LDA	RDDEBUG
	ORA	A
	JZ	DRETURN

	PUSH	H
	PUSH    PSW		The .PRINT will change A
	LXI	H,MPAGE
	SCALL   .PRINT
	POP	PSW		Restore A so we can print it
	CALL	PHEXA
	LXI	H,MADDR
	SCALL	.PRINT
	POP	H
	CALL	PHEXHL
	MVI	A,012Q
	SCALL	.SCOUT
	POP	PSW
	RET

