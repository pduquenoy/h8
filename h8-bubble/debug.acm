DPAGE	EQU	*
	PUSH    PSW
	LDA	RDDEBUG
	ORA	A
	JZ	DRETURN

	PUSH	H
	PUSH    PSW		The .PRINT will change A
	LXI	H,MPAGE
	SCALL   .PRINT
	POP	PSW		Restore A so we can print it
	CALL	PHEXA
	LXI	H,MADDR
	SCALL	.PRINT
	POP	H
	CALL	PHEXHL
	MVI	A,012Q
	SCALL	.SCOUT
	POP	PSW
	RET

MLOAD	DB	12Q,'RD: LOAD',212Q
MREAD   DB      12Q,'RD: READ UNI','='+200Q
MSREAD  DB      12Q,'RD: SMALL READ UNI','='+200Q
MBLK    DB      ' BLK','='+200Q
MDEST   DB      ' DEST','='+200Q
MSRC    DB      ' SRC','='+200Q
MCOUNT	DB	' COUNT','='+200Q
MREADR  DB      12Q,'RD: READ REGARDLESS UNI','='+200Q
MWRITE  DB      12Q,'RD: WRITE UNI','='+200Q
MSWRITE DB      12Q,'RD: WRITE UNI','='+200Q
MRDY    DB      12Q,'RD: READY',212Q
MMNT    DB      12Q,'RD: MOUNT',212Q
MPAGE   DB      '    PAGE','='+200Q
MADDR   DB      ' ADDR','='+200Q
MBACK	DB	'BACK',212Q
MCOPD	DB	'COPIED',212Q

MBIGR	DB	'RD: BIGREAD',212Q
MBIGW	DB	'RD: BIGWRITE',212Q

