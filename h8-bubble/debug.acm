DRETURN	POP	PSW		Generic early return from debug
	RET

DLOAD   EQU	*
 	PUSH    PSW
	LDA	RDDEBUG
	ORA	A
	JZ	DRETURN

	PUSH	H
	LXI	H,MLOAD
	SCALL   .PRINT
	POP	H
	POP	PSW
	RET

DREADR	EQU	*
	PUSH    PSW
	LDA	RDDEBUG
	ORA	A
	JZ	DRETURN

	PUSH	H
	LXI	H,MREADR
	SCALL   .PRINT
	LDA	AIO.UNI
	CALL	PHEXA
	LXI	H,MBLK
	SCALL	.PRINT
	POP	H
	CALL	PHEXHL
	PUSH	H
	LXI	H,MDEST
	SCALL	.PRINT
	CALL    PHEXDE
	LXI	H,MCOUNT
	SCALL   .PRINT
	CALL    PHEXBC
	MVI	A,012Q
	SCALL	.SCOUT
	POP	H
	POP	PSW
	RET

